# Exercise 10-2
# Created in Jupyter Notebook, modified to improve output

def cumsum(list):
    running_total = 0
    new_list = []
    for i in list:
        running_total += int(i)
        new_list.append(running_total)
    return new_list

a = [3,4,5,7]
print("Cumulative sum for each value in list: " + str(a))
print(str(cumsum(a)))

t = [1,2,3]
print("Cumulative sum for each value in list: " + str(t))
print(str(cumsum(t)))
